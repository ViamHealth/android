                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/food_icon"
                                    android:visibility="invisible"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Items"
