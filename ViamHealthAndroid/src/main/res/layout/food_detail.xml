        				    android:text="Sodium"
        				    android:textStyle="bold"
        				    android:textColor="#313131"
        				    android:textSize="12dp"/>
        				<TextView android:id="@+id/sodiumVal"
        				    android:layout_height="wrap_content"
        				    android:layout_width="fill_parent"
