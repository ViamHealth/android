        				<TableRow android:paddingTop="5dp">
        				 <TextView android:id="@+id/ironTxt"
        				    android:layout_height="wrap_content"
        				    android:layout_width="wrap_content"
        				    android:text="Iron"
        				    android:textStyle="bold"
        				    android:textColor="#313131"
        				    android:textSize="12dp"/>
