                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:id="@+id/lblsnakcal"
                                    android:layout_weight="4"
                                    android:textColor="#ffffff"/>
                                <ImageView android:layout_height="wrap_content"
