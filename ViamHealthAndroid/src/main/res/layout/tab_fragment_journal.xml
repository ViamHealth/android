                                android:id="@+id/layout4">
                                <!--
                                <com.viamhealth.android.ui.RefreshableListView
                                    android:layout_width="fill_parent"
                                    android:cacheColorHint="#00000000"
                                    android:layout_weight="1"
                                    android:divider="#000000"
