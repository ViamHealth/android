<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context=".FoodDiary"
    android:orientation="vertical"
    android:background="#EFEFEF" >
    <FrameLayout
        android:id="@+id/main_layout"
        android:layout_height="fill_parent"
        android:layout_width="fill_parent">
        <LinearLayout
            android:id="@+id/actionbarlayot"
            android:layout_height="fill_parent"
            android:layout_width="fill_parent"
            android:orientation="vertical">
            <!-- Action Bar Start -->

            <LinearLayout
                android:id="@+id/food_main_layout"
                android:layout_height="wrap_content"
                android:layout_width="fill_parent"
                android:orientation="vertical"
                >

                <LinearLayout
                    android:id="@+id/food_header"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"

                    >

                    <TextView android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="Food Diary"
                        android:textStyle="bold"
                        android:textColor="#000000"
                        android:textSize="16sp"/>
                    <LinearLayout android:layout_height="fill_parent"
                        android:layout_width="fill_parent"
                        android:orientation="horizontal"
                        android:gravity="right|center_vertical">
                        <LinearLayout
                            android:id="@+id/btn_food_time_picker"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="center_vertical">
                            <TextView android:id="@+id/lbl_food_time"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:text="10:45AM"
                                android:visibility="invisible"
                                android:textColor="#000000"
                                android:textSize="12sp"/>
                            <ImageView
                                android:id="@+id/img_time"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:src="@drawable/picker_bg"/>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/btn_food_date_picker"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:orientation="horizontal"
                            android:gravity="center_vertical">

                            <TextView android:id="@+id/lbl_food_date"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:text="20 March 2013"
                                android:textColor="#000000"
                                android:textSize="12sp"
                                />
                            <ImageView
                                android:id="@+id/img_date"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:src="@drawable/picker_bg"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <ScrollView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:splitMotionEvents="false"
                    android:fadingEdgeLength="0dp"
                    android:layout_gravity="center_horizontal">
                    <LinearLayout android:layout_height="fill_parent"
                        android:layout_width="fill_parent"
                        android:orientation="vertical"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:paddingTop="10dp">
                        <LinearLayout android:layout_height="wrap_content"
                            android:layout_width="fill_parent"
                            android:orientation="vertical">
                            <LinearLayout android:layout_height="wrap_content"
                                android:layout_width="fill_parent"
                                android:background="#37AA4F"
                                android:id="@+id/breakfast"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">
                                  <ImageView android:layout_height="wrap_content"
                                     android:layout_width="wrap_content"
                                     android:padding="5dp"
                                     android:id="@+id/img_breakfast"
                                     android:src="@drawable/picker_bg_1"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:text="Breakfast(0)"
                                    android:textSize="16sp"
                                    android:id="@+id/lblbrk"
                                    android:paddingLeft="5dp"
                                    android:textStyle="bold"
                                    android:textColor="#ffffff"
                                    android:layout_weight="4"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:text="0"
                                    android:gravity="center"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:id="@+id/lbltotalbrkcal"
                                    android:layout_weight="4"
                                    android:textColor="#ffffff"/>
                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:src="@drawable/addicon"
                                    android:id="@+id/addBreakfast"
                                    android:padding="5dp"
                                    android:layout_weight="1"/>
                            </LinearLayout>
                            <LinearLayout android:layout_height="wrap_content"
                                android:layout_width="fill_parent"
                                android:background="#E6F5FF"
                                android:padding="5dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:visibility="gone">
                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/food_icon"
                                    android:visibility="invisible"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Items"
                                    android:layout_weight="1"
                                    android:textColor="#666666"
                                    android:id="@+id/lblitem1"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Servings"
                                    android:layout_weight="1"
                                    android:paddingRight="5dp"
                                    android:textColor="#666666"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Calories"
                                    android:gravity="center"
                                    android:paddingRight="10dp"
                                    android:layout_weight="1"
                                    android:textColor="#666666"/>
                            </LinearLayout>
                            <ScrollView
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:paddingTop="5dp"
                                android:orientation="vertical"
                                android:id="@+id/layout1">
                                <!-- <ListView
                                     android:layout_width="fill_parent"
                                     android:cacheColorHint="#00000000"
                                     android:divider="#000000"
                                     android:visibility="gone"
                                     android:dividerHeight="1dp"
                                     android:headerDividersEnabled="false"
                                     android:footerDividersEnabled="false"
                                     android:layout_height="200dp"
                                     android:id="@+id/lstViewBreakfast" />
                                     <com.viamhealth.android.ui.RefreshableListView
                                    android:layout_width="fill_parent"
                                    android:cacheColorHint="#00000000"
                                    android:layout_weight="1"
                                    android:divider="#000000"
                                    android:dividerHeight="1dp"
                                    android:visibility="gone"
                                    android:headerDividersEnabled="false"
                                    android:footerDividersEnabled="false"
                                    android:layout_height="wrap_content"
                                    android:fastScrollEnabled="true"
                                    android:listSelector="@drawable/list_item_selector"
                                    android:id="@+id/lstViewBreakfast" />
                                      -->
                                     <FrameLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:layout_gravity="center"
                                    android:layout_margin="20dp"
                                    android:background="#ffffff"
                                    android:id="@+id/lstViewBreakfast"/>

                            </LinearLayout>
                            </ScrollView>
                        </LinearLayout>

                        <LinearLayout android:layout_height="wrap_content"
                            android:layout_width="fill_parent"
                            android:orientation="vertical">
                            <LinearLayout android:layout_height="wrap_content"
                                android:layout_width="fill_parent"
                                android:background="#37AA4F"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:id="@+id/lunch">
                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:padding="5dp"
                                    android:src="@drawable/picker_bg_1"
                                    android:id="@+id/img_lunch"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:text="Lunch(0)"
                                    android:id="@+id/lbllunch"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:layout_weight="4"
                                    android:paddingLeft="5dp"
                                    android:textColor="#ffffff"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:text="0"
                                    android:gravity="center"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:id="@+id/lbllunchcal"
                                    android:layout_weight="4"
                                    android:textColor="#ffffff"/>
                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:src="@drawable/addicon"
                                    android:id="@+id/addLunch"
                                    android:padding="5dp"
                                    android:layout_weight="1"/>
                            </LinearLayout>
                            <LinearLayout android:layout_height="wrap_content"
                                android:layout_width="fill_parent"
                                android:background="#E6F5FF"
                                android:gravity="center_vertical"
                                android:padding="5dp"
                                android:orientation="horizontal"
                                android:visibility="gone">
                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/food_icon"
                                    android:visibility="invisible"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Items"
                                    android:id="@+id/lblitem2"
                                    android:layout_weight="1"
                                    android:textColor="#666666"/>
                                <TextView android:layout_height="wrap_content"
