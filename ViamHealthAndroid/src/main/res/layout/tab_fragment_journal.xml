                                    android:layout_gravity="center"
                                    android:layout_margin="20dp"
                                    android:background="#ffffff"
                                    android:id="@+id/lstViewSnakes"/>


                            </LinearLayout>
                        </LinearLayout>


                        <LinearLayout android:layout_height="wrap_content"
                            android:layout_width="fill_parent"
                            android:orientation="vertical">
                            <LinearLayout android:layout_height="wrap_content"
                                android:layout_width="fill_parent"
                                android:background="#37AA4F"
                                android:gravity="center_vertical"
                                android:id="@+id/dinner"
                                android:orientation="horizontal">
                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:padding="5dp"
                                    android:src="@drawable/picker_bg_1"
                                    android:id="@+id/img_dinner"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:text="Dinner(0)"
                                    android:textSize="16sp"
                                    android:paddingLeft="5dp"
                                    android:textStyle="bold"
                                    android:layout_weight="4"
                                    android:id="@+id/lbldinner"
                                    android:textColor="#ffffff"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:text="0"
                                    android:gravity="center"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:id="@+id/lbldinnercal"
                                    android:layout_weight="4"
                                    android:textColor="#ffffff"/>
                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:src="@drawable/addicon"
                                    android:id="@+id/addDinner"
                                    android:padding="5dp"
                                    android:layout_weight="1"/>
                            </LinearLayout>
                            <LinearLayout android:layout_height="wrap_content"
                                android:layout_width="fill_parent"
                                android:background="#E6F5FF"
                                android:padding="5dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal"
                                android:visibility="gone">

                                <ImageView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:src="@drawable/food_icon"
                                    android:visibility="invisible"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Items"
                                    android:id="@+id/lblitem4"
                                    android:layout_weight="1"
                                    android:textColor="#666666"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="0dp"
                                    android:text="Servings"
                                    android:paddingRight="5dp"
                                    android:layout_weight="1"
                                    android:textColor="#666666"/>
