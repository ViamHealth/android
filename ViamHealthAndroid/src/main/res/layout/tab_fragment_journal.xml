                                    android:layout_weight="1"
                                    android:textColor="#666666"/>
                                <TextView android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Calories"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:paddingRight="10dp"
                                    android:textColor="#666666"/>
                            </LinearLayout>
